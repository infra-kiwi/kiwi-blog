{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["es2020"],
    "module": "es2022",
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "strict": true,
    "target": "es2022",
    "types": ["node"],
    "outDir": "dist",
    "listEmittedFiles": true
  },
  "exclude": ["node_modules", "dist"]
}
